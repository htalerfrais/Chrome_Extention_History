{
    "manifest_version": 3,
    "name": "HistoricAI",
    "version": "0.1",
    "description": "Collecte et analyse ton historique Chrome",
    "permissions": [
      "history",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/Engrave128.png",
        "48" : "icons/Engrave48.png",
        "16" : "icons/Engrave48.png",
    }
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }